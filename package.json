{
  "name": "scss-lint-html-reporter",
  "version": "0.2.1",
  "description": "SCSS Lint HTML Reporter with optional TeamCity integration",
  "main": "./src/index.js",
  "bin": "./bin/reporter.js",
  "scripts": {
    "test": "istanbul cover _mocha",
    "coveralls": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/edendramis/scss-lint-html-reporter.git"
  },
  "keywords": [
    "scss",
    "team city",
    "lint"
  ],
  "author": {
    "name": "Evangelia Dendramis",
    "email": "edendramis@gmail.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/edendramis/scss-lint-html-reporter/blob/master/LICENSE"
  },
  "bugs": {
    "url": "https://github.com/edendramis/scss-lint-html-reporter/issues"
  },
  "homepage": "https://github.com/edendramis/scss-lint-html-reporter",
  "dependencies": {
    "hairballs": "0.0.7"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  }
}
